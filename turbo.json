{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "cache": true,
        "outputs": [".next/**", "!.next/cache/**", "dist/**"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "start": {
        "cache": false,
        "dependsOn": ["build"]
      }
    }
  }
  